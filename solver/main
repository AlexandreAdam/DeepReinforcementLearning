

from dynamic4 import DynamicGame
from solver.Node import Node
from solver.Solver import Solver
import numpy as np

game = DynamicGame()

game.step(38)
game.step(37)
game.step(31)
game.step(30)
game.step(24)
game.step(23)
game.step(39)
game.step(36)
game.step(32)
game.step(29)
game.step(25)
game.step(22)

print(game)

solver = Solver(game.gameState)
print(solver.negamax(solver.root_node, -float('inf'), float('inf')))
